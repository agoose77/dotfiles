	name = Angus Hollands

[pull]
    rebase = false

[submodule]
    recurse = true

[filter "lfs"]
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f

[init]
  defaultBranch = main

[core]
	autocrlf = input
	excludesfile = ~/.config/git/gitignore_global
